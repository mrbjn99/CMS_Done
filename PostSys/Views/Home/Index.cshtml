			<script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
			<script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
			<![endif]-->
		<!-- Meta -->
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
		<meta http-equiv="X-UA-Compatible" content="IE=edge" />

		<!-- Favicon  -->
		<link rel="icon" href="~/images/customer - logo.png">
		<!-- fontawesome icon -->
		<link rel="stylesheet" href="~/assets/fonts/fontawesome/css/fontawesome-all.min.css">
		<!-- animation css -->
		<link rel="stylesheet" href="~/assets/plugins/animation/css/animate.min.css">
		<!-- vendor css -->
		<link rel="stylesheet" href="~/assets/css/style.css">
	</head>

	<body>
		<!-- [ Animate ] start -->
		<div class="loader-bg">
			<div class="loader-track">
				<div class="loader-fill"></div>
			</div>
		</div>
		<!-- [ Animate ] End -->
		<!-- Preloader -->
		<div class="spinner-wrapper">
			<div class="spinner">
				<div class="bounce1"></div>
				<div class="bounce2"></div>
				<div class="bounce3"></div>
			</div>
		</div>
		<!-- end of preloader -->
		<!-- [ navigation menu ] start -->
		<nav class="pcoded-navbar">
			<div class="navbar-wrapper">
					<div class="navbar-content scroll-div" style="overflow: hidden; width: 100%; height: calc(100vh - 70px);">
						<div class="navbar-brand header-logo">
							<a href="~/Home/Index" class="b-brand">
								<div class="b-bg">
									<i class="feather icon-circle"></i>
								</div>
								<span class="b-title">CMS</span>
							</a>
							<a class="mobile-menu" id="mobile-collapse" href="javascript:"><span></span></a>
						</div>
						<div class="navbar-content scroll-div">
							<ul class="nav pcoded-inner-navbar">
								<li class="nav-item pcoded-trigger">
									<a href="~/Home/Index" class="nav-link "><span class="pcoded-micon"><i class="feather icon-home"></i></span><span class="pcoded-mtext">Home Page</span></a>
								</li>
								<li class="nav-item pcoded-menu-caption">
									<label>Admin Features</label>
								</li>
								<li>
									<a href="~/Account/Register" class="nav-link "><span class="pcoded-micon"><i class="feather icon-user-plus"></i></span><span class="pcoded-mtext">Create New Account</span></a>
								</li>
								<li>
									<a href="~/Admins/Index" class="nav-link "><span class="pcoded-micon"><i class="feather icon-list"></i></span><span class="pcoded-mtext">List Accounts</span></a>
								</li>
							</ul>
							<ul class="nav pcoded-inner-navbar">
								<li class="nav-item pcoded-menu-caption">
									<label>Login as: Admin</label>
								</li>
								<li>
									<a href="~/Home/Index" class="nav-link"><span class="pcoded-micon"><i class="feather icon-user"></i></span><span class="pcoded-mtext">@Html.DisplayName("Hello " + User.Identity.GetUserName() + "!")</span></a>
								</li>
								@using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm", @class = "nav pcoded-inner-navbar" }))
								{

									<li>
										@Html.AntiForgeryToken()
										<a href="javascript:document.getElementById('logoutForm').submit()" class="nav-link"><span class="pcoded-micon"><i class="feather icon-log-out"></i></span><span class="pcoded-mtext">Log Out</span></a>
									</li>
								}
							</ul>
					</div>
				</div>
			</div>
		</nav>
		<!-- [ navigation menu ] end -->
		<!-- [ Header ] start -->
		<header class="navbar pcoded-header navbar-expand-lg navbar-light">
			<div class="m-header">
				<a class="mobile-menu" id="mobile-collapse1" href="javascript:"><span></span></a>
				<a href="index.html" class="b-brand">
					<div class="b-bg">
						<i class="feather icon-trending-up"></i>
					</div>
					<span class="b-title">CMS</span>
				</a>
			</div>
			<a class="mobile-menu" id="mobile-header" href="javascript:">
				<i class="feather icon-more-horizontal"></i>
			</a>
			<div class="collapse navbar-collapse">
				<ul class="navbar-nav mr-auto">
					<li><a href="javascript:" class="full-screen" onclick="javascript:toggleFullScreen()"><i class="feather icon-maximize"></i></a></li>
				</ul>
			</div>
		</header>
		<!-- [ Header ] end -->
		<!-- [ Main Content ] start -->
		<div class="pcoded-main-container">
			<div class="pcoded-wrapper">
				<div class="pcoded-content">
					<div class="pcoded-inner-content">
						<!-- [ breadcrumb ] start -->
						<div class="page-header">
							<div class="page-block">
								<div class="row align-items-center">
									<div class="col-md-12">
										<ul class="breadcrumb">
											<li class="breadcrumb-item"><a href="~/Home/Index"><i class="feather icon-home"></i></a></li>
										</ul>
										<div class="page-header-title">
											<h2 class="m-b-10">
												@Html.DisplayName("Welcome back,  " + User.Identity.GetUserName() + "!")
											</h2>
										</div>
										<br />
										<br />
										<br />
										<br />
										<div class="row">
											<div class="col-md-6 col-lg-4">
												<div class="card" style="border-radius:5%">
													<img src="https://media.wired.com/photos/5e754badca6a510009874664/master/pass/OpEd-Bill-Gates-1183210445.jpg" alt="Lamp" style="border-radius:5%">
													<div class="card-header">
														<h5><strong>Bill Gates - Co-Founder of Microsoft</strong></h5>
													</div>
													<div class="card-body">
														<p class="card-text">
															“It’s fine to celebrate success but it is more important to heed the lessons of failure.”
														</p>
													</div>
												</div>
											</div>
											<div class="col-md-6 col-lg-4">
												<div class="card" style="border-radius:5%">
													<img src="https://cafebiz.cafebizcdn.vn/2017/photo-1-1488915659086.jpg" alt="Lamp" style="border-radius:5%">
													<div class="card-header">
														<h5><strong>Mark Zuckerberg - Co-Founder of Facebook</strong></h5>
													</div>
													<div class="card-body">
														<p class="card-text">
															“The biggest risk is not taking any risk…
															In a world that’s changing really quickly, the only strategy that is guaranteed to fail, is not taking risks.”
														</p>
													</div>
												</div>
											</div>
											<div class="col-md-6 col-lg-4">
												<div class="card" style="border-radius:5%">
													<img src="https://vnwriter.net/wp-content/uploads/2016/10/jack-ma.jpg" alt="Lamp" style="border-radius:5%">
													<div class="card-header">
														<h5><strong>Jack Ma - Founder of Alibaba</strong></h5>
													</div>
													<div class="card-body">
														<p class="card-text">
															“Customers should be number one, Emplojayees number two, and then only your Shareholders come at number three.”
														</p>
													</div>
												</div>
											</div>
										</div>
									</div>
							</div>
						</div>
						<!-- [ breadcrumb ] end -->
					</div>
				</div>
			</div>
			</div>
		</div>
		<!-- [ Main Content ] end -->
		<!-- Required Js -->
		<script src="~/assets/js/vendor-all.min.js"></script>
		<script src="~/assets/plugins/bootstrap/js/bootstrap.min.js"></script>
		<script src="~/assets/js/pcoded.min.js"></script>
	</body>
</html>
}

